{
    "name": "ksmz/nana",
    "description": "Yet another guzzle wrapper",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "~6.3",
        "spatie/macroable": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "laravel/lumen-framework": "5.*",
        "psy/psysh": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "ksmz\\nana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ksmz\\nana\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "boot-lumen": "@php -S localhost:8888 ./Tests/Fixtures/public/lumen.php"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "ksmz",
            "email": "ksmz@amatsuka.me"
        }
    ]
}
